{
	version		4.0;
	format		ascii;
	class		dictionary;
	location	"system";
	object		topoSetDict;
}

actions (		//;

//heater0
{

    name		heater0_porous_media;

    action		new;

    type		cellSet;

    source		surfaceToCell;

    sourceInfo
    {

        file		"constant/triSurface/heater0_porous_media.stl";

        useSurfaceOrientation		true;

        outsidePoints		((0 0 0));

        includeCut		false;

        includeInside		true;

        includeOutside		false;

        nearDistance		-1;

        curvature		0;

    }

}

